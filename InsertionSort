#include <iostream>

using namespace std;

int main()
{
    int n;
cout<<"Enter the size of array\n";
cin>>n;

int *arr=new int[n];

cout<<"Enter the values of array";
for(int i=0;i<n;i++)
{
    cin>>arr[i];


}
cout<<"Now sorted array using Insert sorting is :\n";

for(int i=1;i<n;i++)
{
   int j=i-1;
   int x=arr[i];

  while(j>-1 &&arr[j]>x)
  {
      arr[j+1]=arr[j];
      j--;
  }
  arr[j+1]=x;

}


for(int i=0;i<n;i++)
{
    cout<<arr[i];
}


}


